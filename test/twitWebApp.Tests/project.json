{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "twitWebApp": {
            "target": "project"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": { "type": "platform", "version": "1.0.1" },
                "TweetinviAPI": "1.2.0",
                "Nancy": "2.0.0-barneyrubble"
            }
        }
    },
    "buildOptions": {
